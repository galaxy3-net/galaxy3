$!/usr/bin/env bash

ls -dl /repos/blueprints 2>/dev/null || su - thedr -c bash -c 'cd /repos ; git clone blueprints:galaxy3-net/blueprints.git'
cd /repos/blueprints && git pull

ls -dl /repos/quarren 2>/dev/null || su - thedr -c bash -c 'cd /repos ; git clone quarren:galaxy3-net/quarren.git'
cd /repos/quarren && git pull