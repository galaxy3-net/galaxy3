#!/usr/sbin/env bash

[ -z ${G3HOME} ] && export G3HOME=$(dirname ${0})
export GIT_HUB="https://github.com/galaxy3-net"
#export GIT_SSH_COMMAND="ssh -F ~/galaxy3/spicerack/.ssh/config"

export SVCROOT=${G3HOME}/svc/

# env | egrep GIT_SSH_COMMAND

cat <<EOD

  G3HOME  -  ${G3HOME}
  GIT_HUB -  ${GIT_HUB}

EOD